# LLM agent (Cohere)
COHERE_API_KEY=your_cohere_api_key
# Optional: override model name used by Cohere agent
COHERE_MODEL=command-a-03-2025

# Router (Gemini)
GEMINI_API_KEY=your_gemini_api_key
# Optional: override Gemini model used for routing
GEMINI_MODEL=gemini-2.0-flash

# Router mode: rule | llm (optional; defaults to llm if GEMINI_API_KEY is set)
ROUTER_MODE=llm
